  "version": "0.2.0",
  "configurations": [
    {
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "name": "Launch localhost",
      "url": "http://localhost:3000/",
      "webRoot": "${workspaceFolder}",
      "pathMappings": [
        {
          "url": "file:///C:/Users/Shinjuro/Desktop/Home%20Project/my-app/src",
          "path": "${workspaceFolder}/src"
        }
      ]
    },
    {
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "name": "Launch index.js",
      "file": "${workspaceFolder}\\src\\index.js"
    },
    {},
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\src\\index.js"
    }
  ]
}
